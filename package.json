{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby, Storybook and Tailwind",
  "version": "1.0.0",
  "author": "Sascha Metz <@smetzdev>",
  "dependencies": {
    "autoprefixer": "^9.8.6",
    "gatsby": "^2.24.50",
    "gatsby-image": "^2.4.16",
    "gatsby-plugin-manifest": "^2.4.24",
    "gatsby-plugin-offline": "^3.2.24",
    "gatsby-plugin-postcss": "^2.3.11",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sharp": "^2.6.28",
    "gatsby-source-filesystem": "^2.3.25",
    "gatsby-transformer-sharp": "^2.5.13",
    "postcss-import": "^12.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "tailwindcss": "^1.7.5"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@storybook/addon-a11y": "^6.0.20",
    "@storybook/addon-actions": "^6.0.20",
    "@storybook/addon-essentials": "^6.0.20",
    "@storybook/addon-links": "^6.0.20",
    "@storybook/addon-storysource": "^6.0.20",
    "@storybook/react": "^6.0.20",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-preset-react-app": "^9.1.2",
    "prettier": "2.1.0",
    "react-is": "^16.13.1"
  },
  "keywords": [
    "gatsby",
    "gatsby-starter",
    "storybook",
    "tailwind"
  ],
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "storybook": "NODE_ENV=production start-storybook -p 6006",
    "build-storybook": "NODE_ENV=production build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/smetzdev/gatsby-starter-storybook-tailwind"
  },
  "bugs": {
    "url": "https://github.com/smetzdev/gatsby-starter-storybook-tailwind/issues"
  }
